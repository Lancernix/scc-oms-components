{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "components/*": ["src/components/*"],
      "utils/*": ["src/utils/*"],
      "hooks/*": ["src/hooks/*"],
      "scc-oms-components": ["src"]
    },
    "strict": true,
    "strictNullChecks": false,
    "declaration": true,
    "declarationDir": "./dist",
    "declarationMap": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [".dumirc.ts", "src/**/*"]
}
