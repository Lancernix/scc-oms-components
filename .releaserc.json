{
  "branches": [
    { "name": "master", "channel": "latest", "range": ">=0.6.0" },
    { "name": "0.x", "channel": "0.x", "range": "^0.6.0" },
    { "name": "beta-0.x", "prerelease": "beta-0.x", "range": "^0.6.0" },
    { "name": "beta-1.x", "prerelease": "beta-1.x", "range": "^1.0.0" }
  ],
  "tagFormat": "v${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/npm", { "tarballDir": "dist" }],
    [
      "@semantic-release/git",
      {
        "assets": ["package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]"
      }
    ],
    "@semantic-release/github"
  ]
}
