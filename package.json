{
  "name": "scc-oms-components",
  "type": "module",
  "version": "0.6.0",
  "packageManager": "pnpm@8.15.9",
  "volta": {
    "node": "24.13.0",
    "pnpm": "8.15.9"
  },
  "description": "Antd components for SCC React Projects",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      }
    },
    "./locale/*": {
      "types": "./esm/locale/zh-CN.d.ts",
      "import": "./esm/locale/*.js",
      "require": "./cjs/locale/*.js"
    }
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/scc-oms-components.min.js",
  "types": "esm/index.d.ts",
  "files": ["cjs", "dist", "esm"],
  "sideEffects": ["dist/*"],
  "engines": {
    "node": "^16.0.0 || ^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "cm": "git-cz",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es",
    "lint:es": "biome lint ./src",
    "format": "biome format ./src",
    "prepare": "husky install && dumi setup",
    "start": "npm run dev",
    "sync:sfyun": "node scripts/sync-sfyun.js"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.8.2",
    "@atlaskit/pragmatic-drag-and-drop": "^1.3.0",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "antd": "^4.24.16",
    "dayjs": "^1.11.11",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "rc-picker": "~2.7.6",
    "react": ">=16.9.0 <19.0.0",
    "react-dom": ">=16.9.0 <19.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.8",
    "lodash-es": "^4.17.21",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.2",
    "@atlaskit/pragmatic-drag-and-drop": "^1.3.0",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.5",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^16.14.56",
    "@types/react-dom": "^16.9.24",
    "antd": "^4.24.16",
    "chalk": "^5.6.2",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "dayjs": "^1.11.11",
    "dumi": "^2.2.17",
    "execa": "^9.6.1",
    "father": "^4.4.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.15",
    "lint-staged": "^15.2.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "rc-picker": "~2.7.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "semantic-release": "^25.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylis": "^4.3.1"
  },
  "lint-staged": {
    "**.{js,ts,cjs,mjs,jsx,tsx,json,jsonc}": [
      "biome lint --apply --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-config.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lancernix/scc-oms-components.git"
  },
  "bugs": {
    "url": "https://github.com/Lancernix/scc-oms-components/issues"
  },
  "homepage": "https://github.com/Lancernix/scc-oms-components#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "authors": ["lancernix@gmail.com"],
  "maintainers": ["lancernix@gmail.com", "18829598229@163.com"]
}
