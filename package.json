{
  "name": "scc-oms-components",
  "type": "module",
  "version": "0.6.0-beta.6",
  "packageManager": "pnpm@8.11.0",
  "description": "Antd components for SCC React Projects",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      }
    },
    "./locale/*": {
      "import": "./esm/locale/*.js",
      "require": "./cjs/locale/*.js"
    }
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/scc-oms-components.min.js",
  "types": "esm/index.d.ts",
  "files": ["cjs", "dist", "esm"],
  "sideEffects": ["dist/*"],
  "engines": {
    "node": "^16.0.0 || ^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "cm": "git-cz",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "biome lint ./src",
    "format": "biome format ./src",
    "prepare": "husky install && dumi setup",
    "start": "npm run dev",
    "release": "node ./release.js"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.8.0",
    "@atlaskit/pragmatic-drag-and-drop": "^1.3.0",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "antd": "~4.22.0",
    "dayjs": "^1.11.11",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "rc-picker": "~2.6.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.8",
    "lodash-es": "^4.17.21",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.1",
    "@atlaskit/pragmatic-drag-and-drop": "^1.3.0",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/inquirer": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^16.14.56",
    "@types/react-dom": "^16.9.24",
    "antd": "~4.22.8",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "dayjs": "^1.11.11",
    "dumi": "^2.2.17",
    "father": "^4.4.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.15",
    "lint-staged": "^15.2.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylis": "^4.3.1"
  },
  "lint-staged": {
    "**.{js,ts,cjs,mjs,jsx,tsx,json,jsonc}": [
      "biome lint --apply --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-config.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "authors": ["lancernix@gmail.com"],
  "maintainers": ["lancernix@gmail.com", "18829598229@163.com"]
}
